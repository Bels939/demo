<odoo>
        <record id="hr_hospital_library_doctor_list" model="ir.ui.view">
            <field name="name">hr.hospital.library.doctor.list</field>
            <field name="model">hr.hospital.library.doctor</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                    <field name="gender"/>
                    <field name="specialisation"/>
                    <field name="category"/>
                    <field name="age"/>
                    <field name="comment"/>
                </list>
            </field>
        </record>

        <record id="hr_hospital_library_doctor_form" model="ir.ui.view">
            <field name="name">hr.hospital.library.doctor.form ()</field>
            <field name="model">hr.hospital.library.doctor</field>
            <field name="arch" type="xml">
                <form>
                    <header></header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="gender"/>
                                <field name="specialisation"/>
                                <field name="category"/>
                                <field name="age"/>
                                <field name="comment"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_hr_hospital_library_doctor_window" model="ir.actions.act_window">
            <field name="name">Doctor</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.hospital.library.doctor</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="hr_hospital_library_menu_doctor"
                  action="action_hr_hospital_library_doctor_window"
                  parent="hr_hospital_library_menu_catalog"
                  name="Doctor" sequence="200"/>

</odoo>